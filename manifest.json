{
	"name": "Dumbwaiter",
	"version": "1",
	"manifest_version": 2,
	"description": "Take data from one page and move it to another.",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"permissions": [
		"tabs", "http://*/", "https://*/"
	],
	"web_accessible_resources": ["jquery.js"],
	"content_scripts": [
		{
	      "matches": ["http://*/*","https://*/*"],
	      "js": ["jquery.js"]
		}
	],
	"options_page": "options.html"
}